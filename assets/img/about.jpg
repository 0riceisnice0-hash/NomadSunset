<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1600 1200" width="1600" height="1200" preserveAspectRatio="xMidYMid slice">
  <defs>
    <!-- Deep cosmic gradient -->
    <radialGradient id="aboutBg" cx="50%" cy="50%">
      <stop offset="0%" style="stop-color:#0f1229;stop-opacity:1" />
      <stop offset="60%" style="stop-color:#080a13;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#05060b;stop-opacity:1" />
    </radialGradient>
    
    <!-- Golden glow -->
    <radialGradient id="goldGlow" cx="50%" cy="50%">
      <stop offset="0%" style="stop-color:#f6e6a6;stop-opacity:0.25" />
      <stop offset="40%" style="stop-color:#d6b45a;stop-opacity:0.12" />
      <stop offset="80%" style="stop-color:#9d7b2f;stop-opacity:0.04" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:0" />
    </radialGradient>
    
    <!-- Aurora accent -->
    <radialGradient id="auroraGlow" cx="30%" cy="70%">
      <stop offset="0%" style="stop-color:#48ffc8;stop-opacity:0.1" />
      <stop offset="50%" style="stop-color:#2a8d6f;stop-opacity:0.05" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Base -->
  <rect width="1600" height="1200" fill="url(#aboutBg)"/>
  
  <!-- Sacred geometry - Flower of Life inspired -->
  <g opacity="0.08" stroke="#d6b45a" stroke-width="1.5" fill="none">
    <!-- Central circle -->
    <circle cx="800" cy="600" r="120"/>
    
    <!-- Six petals around center -->
    <circle cx="800" cy="480" r="120"/>
    <circle cx="904" cy="540" r="120"/>
    <circle cx="904" cy="660" r="120"/>
    <circle cx="800" cy="720" r="120"/>
    <circle cx="696" cy="660" r="120"/>
    <circle cx="696" cy="540" r="120"/>
    
    <!-- Outer layer -->
    <circle cx="800" cy="360" r="120"/>
    <circle cx="1008" cy="480" r="120"/>
    <circle cx="1008" cy="720" r="120"/>
    <circle cx="800" cy="840" r="120"/>
    <circle cx="592" cy="720" r="120"/>
    <circle cx="592" cy="480" r="120"/>
  </g>
  
  <!-- Metatron's Cube lines -->
  <g opacity="0.05" stroke="#f6e6a6" stroke-width="1">
    <line x1="800" y1="360" x2="800" y2="840"/>
    <line x1="592" y1="480" x2="1008" y2="720"/>
    <line x1="592" y1="720" x2="1008" y2="480"/>
    <line x1="696" y1="540" x2="904" y2="660"/>
    <line x1="696" y1="660" x2="904" y2="540"/>
  </g>
  
  <!-- Subtle star field -->
  <g opacity="0.3">
    <circle cx="300" cy="200" r="0.8" fill="#f6e6a6"/>
    <circle cx="1200" cy="300" r="0.6" fill="#ffffff"/>
    <circle cx="450" cy="850" r="0.7" fill="#d6b45a"/>
    <circle cx="1100" cy="900" r="0.5" fill="#ffffff"/>
    <circle cx="200" cy="600" r="0.6" fill="#f6e6a6"/>
    <circle cx="1400" cy="550" r="0.7" fill="#ffffff"/>
    <circle cx="600" cy="150" r="0.5" fill="#d6b45a"/>
    <circle cx="1300" cy="1000" r="0.6" fill="#ffffff"/>
  </g>
  
  <!-- Glowing orbs -->
  <ellipse cx="800" cy="600" rx="600" ry="450" fill="url(#goldGlow)" opacity="0.5"/>
  <ellipse cx="480" cy="840" rx="400" ry="300" fill="url(#auroraGlow)" opacity="0.6"/>
  
  <!-- Prominent twinkling stars -->
  <g>
    <circle cx="1350" cy="250" r="1.5" fill="#f6e6a6" opacity="0.9">
      <animate attributeName="opacity" values="0.7;1;0.7" dur="3s" repeatCount="indefinite"/>
    </circle>
    <circle cx="1350" cy="250" r="4" fill="#f6e6a6" opacity="0.2"/>
    
    <circle cx="250" cy="400" r="1.2" fill="#ffffff" opacity="0.85">
      <animate attributeName="opacity" values="0.65;0.95;0.65" dur="4s" repeatCount="indefinite"/>
    </circle>
    <circle cx="250" cy="400" r="3.5" fill="#ffffff" opacity="0.15"/>
  </g>
  
  <!-- Subtle vignette -->
  <rect width="1600" height="1200" fill="url(#aboutBg)" opacity="0.15"/>
</svg>
